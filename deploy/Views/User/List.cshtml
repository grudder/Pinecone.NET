@model IEnumerable<Pinecone.Models.User>

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2>用户管理</h2>
            <hr />
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Id)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Sn)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.MemberVisitCount)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ChannelVisitCount)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Channel1VisitCount)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Sn)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.MemberVisitCount)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ChannelVisitCount)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Channel1VisitCount)
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
